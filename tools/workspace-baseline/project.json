{
  "name": "workspace-baseline",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/workspace-baseline/src",
  "projectType": "library",
  "tags": ["scope:tooling", "type:feature"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/workspace-baseline",
        "main": "tools/workspace-baseline/src/index.ts",
        "tsConfig": "tools/workspace-baseline/tsconfig.lib.json",
        "assets": [
          "tools/workspace-baseline/*.md",
          {
            "input": "./tools/workspace-baseline/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./tools/workspace-baseline/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./tools/workspace-baseline",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./tools/workspace-baseline",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "options": {
        "lintFilePatterns": [
          "tools/workspace-baseline/**/*.ts",
          "tools/workspace-baseline/package.json",
          "tools/workspace-baseline/generators.json"
        ]
      }
    },
    "lint-report": {
      "options": {
        "lintFilePatterns": [
          "tools/workspace-baseline/**/*.ts",
          "tools/workspace-baseline/package.json",
          "tools/workspace-baseline/generators.json"
        ]
      }
    },
    "unit-test": {},
    "int-test": {}
  }
}
