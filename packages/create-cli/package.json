{
  "name": "@code-pushup/create-cli",
  "version": "0.80.2",
  "license": "MIT",
  "bin": "src/index.mjs",
  "homepage": "https://github.com/code-pushup/cli/tree/main/packages/create-cli#readme",
  "bugs": {
    "url": "https://github.com/code-pushup/cli/issues?q=is%3Aissue%20state%3Aopen%20type%3ABug%20label%3A\"ðŸ§©%20create-cli\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-pushup/cli.git",
    "directory": "packages/create-cli"
  },
  "keywords": [
    "CLI",
    "Code PushUp",
    "automation",
    "developer tools",
    "code quality",
    "conformance",
    "build tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/index.cjs",
  "module": "./src/index.mjs",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.d.mts",
        "default": "./src/index.mjs"
      },
      "require": {
        "types": "./src/index.d.cts",
        "default": "./src/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./src/*.d.mts",
        "default": "./src/*.mjs"
      },
      "require": {
        "types": "./src/*.d.cts",
        "default": "./src/*.cjs"
      }
    }
  },
  "dependencies": {
    "@code-pushup/nx-plugin": "0.80.2",
    "@code-pushup/utils": "0.80.2"
  },
  "files": [
    "src",
    "!**/*.tsbuildinfo"
  ]
}
