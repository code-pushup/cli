{
  "name": "@code-pushup/models",
  "version": "0.80.2",
  "license": "MIT",
  "description": "Model definitions and validators for the Code PushUp CLI",
  "homepage": "https://github.com/code-pushup/cli/tree/main/packages/models#readme",
  "bugs": {
    "url": "https://github.com/code-pushup/cli/issues?q=is%3Aissue%20state%3Aopen%20type%3ABug%20label%3A\"ðŸ§©%20models\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-pushup/cli.git",
    "directory": "packages/models"
  },
  "keywords": [
    "CLI",
    "Code PushUp",
    "developer tools",
    "code quality",
    "conformance",
    "type definition",
    "validators",
    "models"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/index.cjs",
  "module": "./src/index.mjs",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.d.mts",
        "default": "./src/index.mjs"
      },
      "require": {
        "types": "./src/index.d.cts",
        "default": "./src/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./src/*.d.mts",
        "default": "./src/*.mjs"
      },
      "require": {
        "types": "./src/*.d.cts",
        "default": "./src/*.cjs"
      }
    }
  },
  "dependencies": {
    "zod": "^4.0.5",
    "vscode-material-icons": "^0.1.0"
  },
  "files": [
    "src",
    "!**/*.tsbuildinfo"
  ]
}
