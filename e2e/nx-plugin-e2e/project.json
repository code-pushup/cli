{
  "name": "nx-plugin-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "e2e/nx-plugin-e2e/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["e2e/nx-plugin-e2e/**/*.ts"]
      }
    },
    "e2e": {
      "executor": "@nx/vite:test",
      "options": {
        "configFile": "e2e/nx-plugin-e2e/vite.config.e2e.ts"
      }
    },
    "e2e-verdaccio": {
      "command": "echo this is needed to get dynamic 'start-verdaccio-env' target"
    }
  },
  "implicitDependencies": ["nx-plugin", "test-utils"],
  "tags": ["scope:tooling", "type:e2e"]
}
