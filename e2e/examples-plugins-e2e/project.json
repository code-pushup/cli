{
  "name": "examples-plugins-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/examples-plugins-e2e/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["e2e/examples-plugins-e2e/**/*.ts"]
      }
    },
    "e2e": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "config": "e2e/examples-plugins-e2e/vite.config.e2e.ts",
        "reportsDirectory": "../../coverage/examples/examples-plugins-e2e"
      }
    }
  },
  "implicitDependencies": [
    "core",
    "examples-plugins"
  ],
  "tags": ["scope:core", "type:e2e"]
}
